{
  "name": "AstroInsight",
  "version": "0.1.0",
  "private": true,
  "description": "A web application for astrology insights and personalized horoscopes.",
  "keywords": [
    "astrology",
    "horoscope",
    "personalized predictions",
    "astro-insights"
  ],
  "author": "Your Name",
  "scripts": {
    "dev": "wasp start",
    "build": "wasp build",
    "start:prod": "node .wasp/build/server/src/main.js",
    "db:migrate:dev": "wasp db migrate-dev",
    "db:deploy": "wasp db migrate-deploy",
    "start": "npm run dev",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "jest"
  },
  "dependencies": {
    "wasp": "^0.13.0"
  },
  "devDependencies": {
    "prisma": "^5.1.0",
    "typescript": "^5.1.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "jest": "^29.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
